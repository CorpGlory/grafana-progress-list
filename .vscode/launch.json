{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome against localhost",
      "url": "http://localhost:3000/",
      "sourceMaps": true,
      "pathMapping": {
        "/public/plugins/corpglory-progresslist-panel": "${workspaceFolder}/dist"
      },
      "webRoot": "${workspaceFolder}",
      "sourceMapPathOverrides": {
        "webpack:///./*": "${webRoot}/src/*"
      }
    }
  ]
}
